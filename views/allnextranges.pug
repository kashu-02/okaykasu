doctype html
html(lang="ja")
  head
    include ./_head
  body
    include ./_header

    div(class="container")
      div(class="row")
        div(class="col-xl")
          table(class="table")
            thead
              tr
                th(scope="col") 日時
                th(scope="col") 範囲
            tbody
              each nextrange in allnextranges
                tr
                  td
                    p #{nextrange['date']}
                  td
                    p #{nextrange['nextstagerange']}
             
    include ./_script
